{
  "name": "react-certification-2020",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.3",
    "clsx": "^1.1.1",
    "date-fns": "^2.27.0",
    "html-react-parser": "^1.4.0",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loader-spinner": "^4.0.0",
    "react-player": "^2.9.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "lint:fix": "eslint ./src --ext .js,.jsx --fix",
    "test:ci": "npm install && cd release && CI=true npm test",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "ts:check-types": "tsc",
    "lint": "eslint --color --ext .js,.jsx,.ts,.tsx src",
    "stylelint": "stylelint src/*.css src/**/*.css",
    "verify": "npm run lint && npm run stylelint && npm test -- --watchAll=false",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^10.2.13",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.0.0",
    "stylelint": "^14.0.1",
    "stylelint-config-standard": "^23.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js, jsx, css, json}": [
      "yarn run lint:fix",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
